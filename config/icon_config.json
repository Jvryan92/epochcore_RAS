      "optimize": true,
      "texture_atlas": true
    },
    "saas": {
      "formats": ["svg", "png"],
      "optimize": true
    }
  },
  "processing_options": {
    "parallel": true,
    "batch_size": 10,
    "optimize_svg": true,
    "optimize_png": true,
    "max_workers": 4
  },
